{
  "name": "mobx-view-model",
  "version": "5.4.3",
  "scripts": {
    "clean": "rimraf dist",
    "lint:check": "eslint . --fix",
    "ts:check": "tsc --noEmit",
    "check": "npm run lint:check && npm run ts:check",
    "prebuild": "npm run clean && npm run check",
    "build:watch": "npm run build && nodemon --watch src --ext ts --exec \"tsc && node ./post-build.mjs\"",
    "build": "tsc && node ./post-build.mjs",
    "pub": "PUBLISH=true pnpm run build",
    "prepublish": "pnpm test",
    "pub:patch": "PUBLISH=true PUBLISH_VERSION=patch pnpm run build",
    "pub:minor": "PUBLISH=true PUBLISH_VERSION=minor pnpm run build",
    "pub:major": "PUBLISH=true PUBLISH_VERSION=major pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "website": "cd website && pnpm start",
    "website:build": "cd website && pnpm build",
    "dev": "pnpm test:watch"
  },
  "keywords": [
    "mobx",
    "react",
    "view-model",
    "view",
    "model",
    "vm",
    "mvvm",
    "mvc"
  ],
  "author": "js2me",
  "license": "MIT",
  "description": "MobX ViewModel power for ReactJS",
  "bugs": {
    "url": "https://github.com/js2me/mobx-view-model/issues"
  },
  "type": "module",
  "homepage": "https://github.com/js2me/mobx-view-model",
  "repository": {
    "type": "git",
    "url": "git://github.com/js2me/mobx-view-model"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "react-simple-loadable": "^2.0.4",
    "yummies": "^3.0.37"
  },
  "peerDependencies": {
    "mobx": "^6.12.4",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.13",
    "@types/react": "^18.3.18",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-istanbul": "^2.1.8",
    "eslint": "^8.57.1",
    "js2me-eslint-config": "^1.0.7",
    "js2me-exports-post-build-script": "^2.0.17",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}